h1 Please #{title}
form(method='POST' action='/login')
	p
		input(name='ck' type='checkbox' value='1')
		input(name='ck' type='checkbox' value='2')
		input(name='ck' type='checkbox' value='3')
		input(name='ck' type='checkbox' value='4')
		input(name='ck' type='checkbox' value='5')

	p
		input(name='rd' type='radio' value='1')
		input(name='rd' type='radio' value='2')
		input(name='rd' type='radio' value='3')
		input(name='rd' type='radio' value='4')
		input(name='rd' type='radio' value='5')
	p
		select(name='se')
			option(value='1') A
			option(value='2') B
			option(value='3') C
	table(align='center' border='1' cellpadding='10' cellspacing='0')
		tr
			th 用户名：
			td
				input(name='username' type='text')
		tr
			th 密码：
			td
				input(name='password' type='password')
		tr
			td
				input(type='reset' value='重置')
			td
				input(type='submit' value='登录')